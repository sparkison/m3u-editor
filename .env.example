APP_NAME="m3u editor"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_TIMEZONE=UTC
APP_URL=http://localhost
APP_PORT=36400

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# ENABLE_POSTGRES=true
# DB_CONNECTION=pgsql
# PG_DATABASE=
# PG_USER=
# PG_PASSWORD=
# DB_CONNECTION=pgsql
# DB_HOST=127.0.0.1
# DB_PORT=5433
# DB_DATABASE=
# DB_USERNAME=
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

FILESYSTEM_DISK=local

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=predis

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# DON'T CHANGE APP KEY - IT'S A FIXED VALUE FOR THIS APP
# Laravel Reverb (WebSockets)
# BROADCAST_CONNECTION=reverb
# REVERB_APP_ID=m3u-editor
# You will need to update the echo.js file and run `npm run build` if you change the app key
# REVERB_APP_KEY=5e2a227aacd3bc04713e595428195896617947b8f5ec11db31029abd13b13538
# Optional: will be auto-generated if not set, but you can set a fixed value if you want to avoid it changing on each deploy
# REVERB_APP_SECRET=
REVERB_HOST=localhost
REVERB_PORT=36800
REVERB_SCHEME=http

# M3U Proxy Configuration (Python-based streaming proxy)
# M3U_PROXY_ENABLED=true means use EMBEDDED proxy (default, runs in container)
# M3U_PROXY_ENABLED=false means use EXTERNAL proxy service (separate container)
# M3U_PROXY_ENABLED=true              # true = embedded, false/null = external service
# M3U_PROXY_HOST=127.0.0.1            # Host for embedded proxy (localhost)
# M3U_PROXY_PORT=8085                 # Port for embedded proxy (internal)

STUCK_PROCESSING_MINUTES=120

#AUTO_RETRY_503_ENABLED=true
#AUTO_RETRY_503_MAX=3
#AUTO_RETRY_503_COOLDOWN_MINUTES=10
#AUTO_RETRY_503_DELAY_MIN_SECONDS=300
#AUTO_RETRY_503_DELAY_MAX_SECONDS=900

# NOTE: Only applies when using embedded proxy. If using external proxy, you must add the variables to the external service instead.
# HLS storage & garbage collection
# HLS_TEMP_DIR: where HLS segments are written (default below)
# HLS_GC_ENABLED: enable background garbage collector (true/false)
# HLS_GC_INTERVAL: GC loop interval in seconds (default 600)
# HLS_GC_AGE_THRESHOLD: delete files older than this many seconds (default 7200)
HLS_TEMP_DIR=/var/www/html/storage/app/hls-segments
HLS_GC_ENABLED=true
HLS_GC_INTERVAL=600
HLS_GC_AGE_THRESHOLD=7200

# Network Broadcasting (pseudo-TV channels)
# NETWORK_BROADCAST_ENABLED: enable the network broadcast service (true/false)
# When enabled, networks with broadcast_enabled=true will stream live HLS content
#NETWORK_BROADCAST_ENABLED=true
